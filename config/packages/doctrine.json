{
  "doctrine": {
    "orm": {
      "auto_generate_proxy_classes": true,
      "naming_strategy": "doctrine.orm.naming_strategy.underscore",
      "auto_mapping": true,
      "mappings": {
        "App": {
          "is_bundle": false,
          "type": "attribute",
          "dir": "%kernel.project_dir%/src/Persistance/Entity",
          "prefix": "App\\Persistance\\Entity",
          "alias": "App"
        }
      }
    }
  }
}